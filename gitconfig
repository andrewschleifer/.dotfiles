[alias]
    amend = commit --amend
    cherrypick = cherry-pick
    co = checkout
    eat = merge --no-ff
    l = log --all --graph --decorate --oneline
    st = status -sb
    w = whatchanged
    unstage = reset HEAD
[color]
    ui = true
[core]
    excludesfile = "~/.dot/gitignore"
[difftool]
    prompt = false
[mergetool]
    prompt = false
